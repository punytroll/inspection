							</bits>
						</length>
					</field>
					<field name="SessionIDLength">
						<type-reference>
							<part>Number</part>
							<part>Integer</part>
							<part>Unsigned</part>
							<part>8Bit</part>
						</type-reference>
					</field>
					<field name="SessionID">
						<type-reference>
							<part>Buffer</part>
							<part>UnsignedInteger_8Bit_EndedByLength</part>
						</type-reference>
						<length>
							<bytes>
								<unsigned-integer-64bit>
									<data-reference root="type">
