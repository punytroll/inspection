						<element-type>
							<type-reference>
								<part>SSL_TLS</part>
								<part>Extension</part>
							</type-reference>
						</element-type>
						<element-name>Extension</element-name>
					</array>
					-->
				</sequence>
			</case>
		</select>
	</sequence>
