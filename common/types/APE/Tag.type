<type>
	<field name="APETagsHeader">
		<type-reference>
			<part>APE</part>
			<part>HeaderOrFooter</part>
		</type-reference>
	</field>
	<field name="APETagsItems">
		<type-reference>
			<part>Array</part>
			<part>EndedByNumberOfElements</part>
		</type-reference>
		<parameters>
			<parameter name="ElementType">
				<type-reference>
					<part>APE</part>
					<part>Item</part>
				</type-reference>
			</parameter>
			<parameter name="ElementName">
				<string>APETagsItem</string>
			</parameter>
			<parameter name="NumberOfElements">
				<data-reference cast-to-type="unsigned integer 64bit">
					<field>APETagsHeader</field>
					<field>ItemCount</field>
				</data-reference>
			</parameter>
		</parameters>
	</field>
	<field name="APETagsFooter">
		<type-reference>
			<part>APE</part>
			<part>HeaderOrFooter</part>
		</type-reference>
	</field>
</type>
