<type>
	<field name="Identifier">
		<type-reference>
			<part>String</part>
			<part>ASCII</part>
			<part>Alphabetic_ByTemplate</part>
		</type-reference>
		<parameters>
			<parameter name="Template">
				<string>APETAGEX</string>
			</parameter>
		</parameters>
	</field>
	<field name="VersionNumber">
		<type-reference>
			<part>Number</part>
			<part>Integer</part>
			<part>Unsigned</part>
			<part>32Bit_LittleEndian</part>
		</type-reference>
		<interpretation>
			<apply-enumeration>
				<enumeration base-type="unsigned integer 32bit">
					<element base-value="1000" valid="true">
						<tag name="interpretation">
							<string>1.000 (old)</string>
						</tag>
					</element>
					<element base-value="2000" valid="true">
						<tag name="interpretation">
							<string>2.000 (new)</string>
						</tag>
					</element>
					<fallback-element valid="false">
						<tag name="interpretation"/>
					</fallback-element>
				</enumeration>
			</apply-enumeration>
		</interpretation>
	</field>
	<field name="TagSize">
		<type-reference>
			<part>Number</part>
			<part>Integer</part>
			<part>Unsigned</part>
			<part>32Bit_LittleEndian</part>
		</type-reference>
	</field>
	<field name="ItemCount">
		<type-reference>
			<part>Number</part>
			<part>Integer</part>
			<part>Unsigned</part>
			<part>32Bit_LittleEndian</part>
		</type-reference>
	</field>
	<field name="TagsFlags">
		<type-reference>
			<part>APE</part>
			<part>Flags</part>
		</type-reference>
	</field>
	<field name="Reserved">
		<type-reference>
			<part>Data</part>
			<part>Unset_EndedByLength</part>
		</type-reference>
		<length>
			<bytes>
				<unsigned-integer-64bit>8</unsigned-integer-64bit>
			</bytes>
			<bits>
				<unsigned-integer-64bit>0</unsigned-integer-64bit>
			</bits>
		</length>
	</field>
</type>
