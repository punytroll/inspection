<type>
	<sequence>
		<field name="IconDirectory">
			<type-reference>
				<part>Microsoft</part>
				<part>Icon</part>
				<part>IconDirectory</part>
			</type-reference>
		</field>
		<array name="ImageData">
			<iterate type="for-each-field">
				<field-reference root="type">
					<field>IconDirectory</field>
					<field>Entries</field>
				</field-reference>
			</iterate>
			<element-name>Image</element-name>
			<element-type>
				<part>Microsoft</part>
				<part>Icon</part>
				<part>ImageData</part>
			</element-type>
		</array>
	</sequence>
</type>
