<type>
	<sequence>
		<field name="ContentType">
			<type-reference>
				<part>Number</part>
				<part>Integer</part>
				<part>Unsigned</part>
				<part>8Bit</part>
			</type-reference>
			<interpretation>
				<apply-enumeration>
					<enumeration base-data-type="unsigned integer 8bit">
						<element base-value="0" valid="false">
							<tag name="interpretation">
								<string>Invalid</string>
							</tag>
						</element>
						<element base-value="20" valid="true">
							<tag name="interpretation">
								<string>ChangeCipherSpec</string>
							</tag>
						</element>
						<element base-value="21" valid="true">
							<tag name="interpretation">
								<string>Alert</string>
							</tag>
						</element>
						<element base-value="22" valid="true">
							<tag name="interpretation">
								<string>Handshake</string>
							</tag>
						</element>
						<element base-value="23" valid="true">
							<tag name="interpretation">
								<string>Application</string>
							</tag>
						</element>
						<element base-value="24" valid="true">
							<tag name="interpretation">
								<string>Heartbeat</string>
							</tag>
						</element>
						<fallback-element valid="false">
							<tag name="interpretation">
								<nothing/>
							</tag>
						</fallback-element>
					</enumeration>
				</apply-enumeration>
			</interpretation>
		</field>
		<field name="ProtocolVersion">
			<type-reference>
				<part>SSL_TLS</part>
				<part>Version</part>
			</type-reference>
		</field>
		<field name="Length">
			<type-reference>
				<part>Number</part>
				<part>Integer</part>
				<part>Unsigned</part>
				<part>16Bit_BigEndian</part>
			</type-reference>
			<verification>
				<less-than>
					<data-reference root="current"/>
					<unsigned-integer-16bit>16384</unsigned-integer-16bit>
				</less-than>
			</verification>
		</field>
		<select>
			<case>
				<when>
					<equals>
						<data-reference root="type">
							<field>ContentType</field>
							<tag>interpretation</tag>
						</data-reference>
						<string>Handshake</string>
					</equals>
				</when>
				<field name="Fragment">
					<type-reference>
						<part>SSL_TLS</part>
						<part>HandshakeFragment</part>
					</type-reference>
					<tag name="protocol">
						<data-reference root="type">
							<field>ContentType</field>
							<tag>interpretation</tag>
						</data-reference>
					</tag>
				</field>
			</case>
		</select>
		<tag name="content">
			<data-reference root="type">
				<field>ContentType</field>
				<tag>interpretation</tag>
			</data-reference>
		</tag>
	</sequence>
</type>
